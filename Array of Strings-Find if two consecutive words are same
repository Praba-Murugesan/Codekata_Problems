    "l=[]\n",
    "c=0\n",
    "for i in range(0,N):\n",
    "    s=input().split()\n",
    "    l.append(s)\n",
    "for i in range(0,N-1):\n",
    "    if(l[i]==l[i+1]):\n",
    "      c=1\n",
    "if(c==1):\n",
    "    print('yes')\n",
