    "n=int(input())\n",
    "l=list(map(int,input().split()))\n",
    "large=0\n",
    "small=0\n",
    "c=max(l)\n",
    "d=min(l)\n",
    "for i in range(0,n):\n",
    "  if(c==l[i]):\n",
    "    large=i\n",
    "  elif(d==l[i]):\n",
    "    small=i\n",
    "print(large-small)"
