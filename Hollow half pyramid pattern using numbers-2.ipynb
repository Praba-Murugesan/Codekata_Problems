   "source": [
    "R=int(input())\n",
    "for i in range(0,R):\n",
    "    l=[]\n",
    "    for j in range(0,i+1):\n",
    "      if(i==R-1):\n",
    "        l.append(str(j+1))\n",
    "      elif(i<R-1 and i>=0):\n",
    "        if(j==0 or j==i):\n",
    "          l.append(str(j+1))\n",
    "        else:\n",
    "          l.append(' ')\n",
    "    print(''.join(l))"
   ]
