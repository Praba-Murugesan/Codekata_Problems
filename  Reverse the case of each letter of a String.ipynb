   "source": [
    "S=input()\n",
    "l=[]\n",
    "if(len(S)<=100):\n",
    "    for i in range(0,len(S)):\n",
    "        if(S[i].isupper()):\n",
    "            l.append(S[i].lower())\n",
    "        elif(S[i].islower()):\n",
    "            l.append(S[i].upper())\n",
    "        else:\n",
    "            l.append(S[i])\n",
    "print(''.join(l))\n"
   ]
  }
