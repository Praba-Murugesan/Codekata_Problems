    "S=input()\n",
    "l=[]\n",
    "N=len(S)\n",
    "if(1<=N<=100000):\n",
    "    for i in range(0,len(S)):\n",
    "        l.append(ord(S[i]))\n",
    "    for i in range(0,len(S)):\n",
    "        l[i]=l[i]+3\n",
    "        if(l[i]>90):\n",
    "          l[i]=l[i]-26\n",
    "        l[i]=chr(int(l[i]))\n",
    "print(''.join(l))"
   ]
