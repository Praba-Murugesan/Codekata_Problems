    "S=input()\n",
    "l=[]\n",
    "for i in range(0,len(S)-1):\n",
    "    count=1\n",
    "    for j in range(i+1,len(S)):\n",
    "        if(S[i]==S[j]):\n",
    "            count=count+1\n",
    "    l.append(count)\n",
    "if(l==[]):\n",
    "    print('0')\n",
    "else:\n",
    "    print(max(l))            "
