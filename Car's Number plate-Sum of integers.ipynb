    "import re\n",
    "s=input()\n",
    "sum=0\n",
    "l=[]\n",
    "c=re.findall('\\d',s)\n",
    "l=list(map(int,c))\n",
    "for i in l:\n",
    "        sum=sum+i\n",
    "print(sum)\n",
