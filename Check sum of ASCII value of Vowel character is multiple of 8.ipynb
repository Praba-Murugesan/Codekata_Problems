    "s=input()\n",
    "l=['a','e','i','o','u']\n",
    "sum=0\n",
    "for i in range(0,len(s)):\n",
    "    if(s[i] in l):\n",
    "        sum=sum+ord(s[i])\n",
    "if(sum%8==0):\n",
    "    print('1')\n",
    "else:\n",
    "    print('0')"
