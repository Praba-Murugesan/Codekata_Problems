    "n=int(input())\n",
    "l=list(map(int,input().split()))\n",
    "k=int(input())\n",
    "s=[]\n",
    "for i in range(0,n-2):\n",
    "  c=i\n",
    "  count=0\n",
    "  for j in range(c,i+k):\n",
    "    if(l[j]<0):\n",
    "      s.append(l[j])\n",
    "      count=1\n",
    "      break\n",
    "  if(count==0):\n",
    "      s.append(0)\n",
    "print(*s)"
   ]
  }
