    "n,m=map(int,input().split())\n",
    "l=list(map(int,input().split()))\n",
    "s=[]\n",
    "for i in range(0,n-1):\n",
    "        \n",
    "      if(abs(l[i]-l[i+1])>m):\n",
    "        s.append(1)\n",
    "      else:\n",
    "        s.append(0)\n",
    "if(abs(l[n-1]-l[0])>m):\n",
    "  s.append(1)\n",
    "else:\n",
    "  s.append(0)\n",
    "\n",
    "print(*s)"
   ]
