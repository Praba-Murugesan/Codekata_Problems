    "S,K,N=list(map(str,input().split()))\n",
    "t=int(N)\n",
    "count=0\n",
    "for i in range(0,len(S)):\n",
    "    if(S[i]==K[i]):\n",
    "        count=count+1\n",
    "if(len(S)-count==t):\n",
    "    print('yes')\n",
    "else:\n",
    "    print('no')"
